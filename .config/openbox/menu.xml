		<menu execute="cat ~/.config/openbox/xdg.xml" id="pipe-455059" label=":2"/>
		<menu execute="~/.config/openbox/obpipemenu-places ~/" id="place" label=":3"/>
		<menu id="conf" label=":4">
			<menu id="conf-165647" label="Files">
				<item label="conky">
					<action name="Execute">
						<execute>
							gedit ~/.conkyrc
						</execute>
					</action>
				</item>
				<item label="dmenu">
					<action name="Execute">
						<execute>
							gedit ~/.config/dmenu/dmenu-bind.sh
						</execute>
					</action>
				</item>
				<item label="terminator">
					<action name="Execute">
						<execute>
							gedit ~/.config/terminator/config
						</execute>
					</action>
				</item>
			</menu>
			<menu id="conf-5174" label="Openbox">
				<item label="GUI menu">
					<action name="Execute">
						<execute>
							obmenu
						</execute>
					</action>
				</item>
				<item label="GUI conf">
					<action name="Execute">
						<execute>
							obconf
						</execute>
					</action>
				</item>
				<item label="menu.xml">
					<action name="Execute">
						<execute>
							gedit ~/.config/openbox/menu.xml
						</execute>
					</action>
				</item>
				<item label="rc.conf">
					<action name="Execute">
						<execute>
							gedit ~/.config/openbox/rc.xml
						</execute>
					</action>
				</item>
				<item label="Reconfigure">
					<action name="Reconfigure"/>
				</item>
				<item label="Restart">
					<action name="Restart"/>
				</item>
			</menu>
			<menu id="conf-634342" label="Tint2">
				<item label="tint2rc">
					<action name="Execute">
						<execute>
							gedit ~/.config/tint2/tint2rc
						</execute>
					</action>
				</item>
				<item label="Restart Tint2">
					<action name="Execute">
						<execute>
							bash ~/.config/tint2/restart_tint2
						</execute>
					</action>
				</item>
			</menu>
			<menu id="conf-483658" label="Network">
				<item label="Menu">
					<action name="Execute">
						<execute>
							terminator -x netcfg-menu
						</execute>
					</action>
				</item>
			</menu>
			<menu id="conf-143947" label="Sound">
				<item label="Mixer">
					<action name="Execute">
						<execute>
							terminator -x alsamixer
						</execute>
					</action>
				</item>
			</menu>
			<item label="screenshot">
				<action name="Execute">
					<execute>
						scrot '%Y-%m-%d_$wx$h_scrot.png'  -e 'mv $f ~/img/' -d 10
					</execute>
				</action>
			</item>
			<item label="Screen">
				<action name="Execute">
					<execute>
						arandr
					</execute>
				</action>
			</item>
			<item label="GUI settings">
				<action name="Execute">
					<execute>
						lxappearance
					</execute>
				</action>
			</item>
			<item label="Power Manager">
				<action name="Execute">
					<execute>
						xfce4-power-manager-settings
					</execute>
				</action>
			</item>
			<item label="Screensaver">
				<action name="Execute">
					<execute>
						xscreensaver-demo
					</execute>
				</action>
			</item>
			<item label="Wallpaper">
				<action name="Execute">
					<execute>
						nitrogen
