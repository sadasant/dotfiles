"xrefresh"
  control+shift+r

# Increase volume
"amixer set Master playback 1+"
   XF86AudioRaiseVolume

# Decrease volume
"amixer set Master playback 1-"
   XF86AudioLowerVolume

# Toggle sound
"amixer set Master toggle"
   XF86AudioMute

# Inc backlight
"xbacklight -inc 5"
   XF86MonBrightnessUp

# Dec backlight
"xbacklight -dec 5"
   XF86MonBrightnessDown
