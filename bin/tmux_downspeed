#!/bin/bash

interface=$(ifconfig | grep "RUNNING" | cut -d':' -f1)
interface=$(echo ${interface//lo/} | cut -d ' ' -f1)

RXB=$(</sys/class/net/$interface/statistics/rx_bytes)
sleep 2
RXBN=$(</sys/class/net/$interface/statistics/rx_bytes)
RXDIF=$(echo $((RXBN - RXB)))

echo -e "$((RXDIF / 1024 / 2))"
